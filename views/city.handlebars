{{> nav }}

<div class="bg">
<h2>They has signed in the city of {{city}}</h2>

{{#each data}}

{{#if url}}
<div class="signers">Name: <a href="http://{{url}}" target="_blank">{{ first_name }} {{ last_name }}</a> {{age}}</div>
{{else}}
<div class="signers">Name: {{ first_name }} {{ last_name }} {{age}}  </div>
{{/if}}
{{/each}}
<div class="button"><a href="/signers"><p>Back</p></a></div>
</div>
